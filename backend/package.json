{
  "name": "mart-az-backend",
  "version": "1.0.0",
  "description": "Backend API for Mart.az - Azerbaijan's classified ads marketplace",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "db:seed": "node src/seeders/index.js",
    "db:sync": "node -e \"require('./src/models/index.js'); require('./src/database/connection').sequelize.sync({ alter: true }).then(() => console.log('Database synchronized successfully')).catch((err) => console.error('Error synchronizing database:', err))\"",
    "db:reset": "node -e \"require('./src/models/index.js'); require('./src/database/connection').sequelize.sync({ force: true }).then(() => console.log('Database reset successfully')).catch((err) => console.error('Error resetting database:', err))\"",
    "db:init": "NODE_ENV=production FORCE_INIT=true node src/database/init.js",
    "db:migrate": "node scripts/run-migrations.js",
    "db:migrate:status": "npx sequelize-cli db:migrate:status",
    "db:create-report-table": "node scripts/force-create-report-table.js",
    "render:db:setup": "NODE_ENV=production RENDER=true node scripts/render-db-setup.js",
    "render:migrate": "NODE_ENV=production RENDER=true node scripts/render-migration.js",
    "emergency:create-tables": "NODE_ENV=production RENDER=true node src/database/init.js",
    "supabase:setup": "NODE_ENV=production USE_SUPABASE=true node scripts/supabase-sync.js",
    "supabase:reset": "NODE_ENV=production USE_SUPABASE=true node scripts/supabase-sync.js --force",
    "supabase:migrate": "NODE_ENV=production USE_SUPABASE=true node scripts/run-migrations.js",
    "supabase:seed": "NODE_ENV=production USE_SUPABASE=true node src/seeders/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^2.6.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/redux-persist": "^4.0.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.8.3",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.14.0",
    "pg-hstore": "^2.3.4",
    "react": "^19.0.0",
    "react-icons": "^5.5.0",
    "redux-persist": "^6.0.0",
    "sequelize": "^6.37.6",
    "sequelize-cli": "^6.6.2",
    "slugify": "^1.6.6",
    "umzug": "^3.8.8",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  }
}
